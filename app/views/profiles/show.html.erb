<div class="row">
  <div class="clod-md-4">
    <div class="panel panel-default">
    <div class="panel-body">

<p>
  <strong>Name: </strong>
  <%= @profile.first_name %>
  <%= @profile.last_name %>
</p>

<p>
  <strong>About Me: </strong>
  <%= @profile.biography %>
</p>

<p>
    <strong>Photo:</strong>
    <%= image_tag(@profile.picture_url(:thumbnail), :width => 100, :height => 100) %>
</p>

<p>
  <strong>Status: </strong>
  <%= current_user.user_type %>
</p>

<% if current_user.profile == @profile %>
  <%= link_to 'Edit', edit_profile_path(@profile), class: 'btn btn-warning' %>
  <% end %>



</div></div></div></div>
